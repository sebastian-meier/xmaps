{
  "name": "xmaps",
  "version": "1.0.0",
  "description": "XMApS",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p tslint.json --force",
    "build": "tsc",
    "webpack": "webpack --display-error-details --progress --colors",
    "typescript": "npm run lint && npm run build && webpack",
    "watch": "nodemon "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-meier/xmaps.git"
  },
  "author": "@seb_meier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebastian-meier/xmaps/issues"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "dst/*"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run typescript",
    "ext": "ts"
  },
  "homepage": "https://github.com/sebastian-meier/xmaps#readme",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@turf/turf": "^5.1.6",
    "@types/d3": "^5.7.2",
    "babel-loader": "^8.3.0",
    "babel-preset-es2015": "^6.24.1",
    "canvg": "^3.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "d3": "^5.14.2",
    "gif.js.optimized": "^1.0.1",
    "leaflet": "^1.6.0",
    "leaflet-search": "^2.9.8",
    "nodemon": "^2.0.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "uuid": "^3.3.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
